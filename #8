#include <iostream>
#include <string>

using namespace std;

int main()
{
    int wnum;
	
    cout << "Enter a number: \n";
    cin >> wnum;
	cout << "Multipication table of: " << wnum << endl;
	
	
    for (float i = 1; i <= 10; ++i) 
	{
        cout << i << " * " << wnum << " = " << wnum * i << endl;
    }
    
    return 0;
}
